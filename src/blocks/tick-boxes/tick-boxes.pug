include ../tick-box/tick-box.pug
include ../section-title/section-title.pug

mixin tick-boxes
    section.tick-boxes
        +section-title({title: "Tick Boxes"})
        .tick-boxes__content
            .tick-boxes__tick-box
                +tick-box
            .tick-boxes__tick-box
                +tick-box({checked: false})
